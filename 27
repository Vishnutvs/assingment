#include<stdio.h>

int main()
{
    int x = 30, y = 40;
    
    if(x == y)
        printf("x is equal to y\n");
    else if(x > y)
        printf("x is greater than y\n");
    else if(x < y)
        printf("x is less than y\n"); // Corrected line with a semicolon at the end
    
    return 0;
}
x is less than y
